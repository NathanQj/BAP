
@font-face
{
	font-family: biko;
	src: url(../font/Biko_Regular.otf);
}

@font-face
{
	font-family: biko_bold;
	src: url(../font/Biko_Bold.otf);
}
@font-face
{
	font-family: biko_black;
	src: url(../font/Biko_Black.otf);
}

body{
	font-family: "biko";
	text-align: center;
	position: relative;
	background-color: #f4f4f4;

}

strong{
	font-weight: bold;
}

.header, .main{
	display: inline-block;
	width: 980px;
	margin: 0 auto;
	text-align: left;
}
/* header */
.header{
	padding-top: 20px;
	padding-bottom: 20px
}
.header div, .header img, .header form{
	float: left;
}

.logo{
	width: 100px;
	border: none;
}
.search{
padding-top: 60px;
margin-left: 50px;
margin-right: 50px;
}

.loupe{
	width: 25px;
	padding-right: 10px;
}

.search input[type=text]{
	width: 500px;
	height: 24px;
	border: 2px solid #25b6ae;
}
.search input[type=submit]{
	background: none;
	border: none;
	color: #25b6ae;
	font-weight: bold;
	cursor: pointer;
}

.header-profil{
	padding-top: 30px;
}

.header-profil img{
	width: 56px;
	height: 56px;
	border: 2px solid #25b6ae;
}

.header-profil a{
	color: #37b6ae;
}

.header-profil p {
	float: left;
	margin-top: 0;
	margin-left: 5px;
}

/* home */

.col1{
	width: 560px;
	float: left;
	margin-right: 20px;
}

.col2{
	float: left;
	width: 380px;
}

.main h3{
	color: #25b6ae;
}

.main h2{
	color: white;
	text-transform: uppercase;
	font-family: "biko_black";
}



.pub1, .pub2{
	width: 100%;
	border: 1px solid black;
	margin-top: 20px;
}

.apprendre .content{
	background: url(../images/apprendre.png) 15px 15px no-repeat #25b6ae;
	background-size: 70px 82px;
}

.infos .content{
	background: url(../images/infos.png) 15px 15px no-repeat #25b6ae;
	background-size: 70px 82px;
}

.create{
	background: url(../images/creer.png) 15px 15px no-repeat #25b6ae;
	background-size: 55px;
	display: inline-block;
	width: 100%;
	padding: 15px 0;
	cursor: pointer;
}

.content{
	display: inline-block;
	width: 100%;
	padding: 15px 0;
	height: 67px;
	cursor: pointer;
}

.content .profil-incomplet{
	padding-left: 125px;
	color: white;
	display: none;
}

.content .profil-incomplet hr {
	border: 1px solid white;
}

.content h2{
	padding-left: 90px; 
	margin-top: 0px;
	margin-bottom: 10px;
	line-height: 20px;
}

.create h2{
	padding-left: 90px; 
	margin-top: 0px;
	margin-bottom: 10px;
	line-height: 20px;
}

.create span{
	padding-left: 90px;
	display: inherit;
	color: white;
	margin-bottom: 10px;
}
.create img{
	width: 340px;
	margin: 0 20px;
}

.populaire h3{
	color: black;
}

.populaire ul.inline-video li img{
	border: 3px solid black;
	width: 90px;
	height: 70px;
}

.historique{
	clear: both;
}

.historique ul.inline-video li img{
	border: 3px solid orange;
	width: 90px;
	height: 70px;
}

ul.inline-video{
	display: inline-block;
	padding: 0;
}

ul.inline-video li{
	float: left;
	width: 96px;
	margin-right: 20px;
	list-style: none;
	font-size: 10px;
	cursor: pointer;
}

ul.inline-video li:last-child{
	margin-right: 0;
}

.profils ul {
	border: 1px solid black;
	background: white;
	padding: 10px 10px 0 10px;
	display: inline-block;
	margin: 0;
}

.profils ul li{
	float: left;
	width: 63px;
	margin-right: 10px;
	list-style: none;
	font-size: 9px;
}

.profils ul li:nth-child(5n){
	margin-right: 0px;
}

.profils ul li img{
	border: 2px solid #25b6ae;
	width: 59px;
}

.likeus{
	height: 210px;
}

.overlay{
	display: none;
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	z-index: 10;
	background-color: rgba(0,0,0,0.5); 
}

.qcm{
	width: 440px;
	position: absolute;
	top: 0;
	left: 50%;
	margin-left: -220px;
	margin-top: 150px;
	background-color: white;
	border: 2px solid #25b6ae;
	z-index: 11;
	text-align: left;
}

.qcm form{
	background: #25b6ae;
}

.qcm .page-number{
	text-transform: uppercase;
	padding: 5px 10px 5px 15px;
	background-color: #25b6ae;
	position: relative;
	display: inline;
	top: 35px;
	left: -10px;
}

.qcm-page{
	display: none;
	background: white;
}

.qcm img{
	border-bottom: 1px solid #25b6ae;
	width: 100%;
	margin-top: -20px;
	max-height: 300px;
}

.qcm h3 {
	margin-left: 15px;
	color: #25b6ae;
}

.qcm .checkbox{
	margin-left: 50px;
	margin-bottom: 20px;
}

.qcm .checkbox input, .inscrit input[type=checkbox]{
	display: none;
}

label.check-style{
	background: url(../images/checkbox.png) no-repeat;
	padding-left: 28px;
	margin-bottom: 10px;
	background-position: 0 2px;
	display: inline-block;
}

input[type="checkbox"]:checked + label.check-style {
 	background-position: 0 -23px; 
 }

.qcm .next-page, .qcm input[type=submit]{
	color: white;
	margin-left: 180px;
	margin-bottom: 20px;
	padding: 5px 10px 5px 10px;
	background-color: #25b6ae;
	text-transform: uppercase;
	border: none;
	display: inline-block;
	cursor: pointer;
}

div.qcm-pres{
	background: #25b6ae;
	color: white;
	text-align: center;
	padding: 20px 20px 30px 20px;
	cursor: pointer;
}

.qcm-pres p{
	text-align: initial;
}

.qcm-pres span{
	float: right;
}

.qcm-pres img{
	width: 110px;
}

.qcm-pres h2 {
	margin: 0 0 40px 0;
}

.qcm-pres .suivant{
	background: url(../images/suivant.png) 100% 70% no-repeat;
	padding-right: 15px;
	background-size: 9px;
	margin-bottom: 10px;
}


/* pre-home */

.logo.pre-home{
	position: relative;
	top: 50px;
	left: 50px;
	float: left;
	z-index: 10;
}

.slider-img{
	z-index: 1;
	position: absolute;
}

.slider{
	width: 600px;
	float: left;
	position: relative;
	overflow: hidden;
	height: 600px;
	margin-left: -100px;
	background: white;

}

.connexion{
	width: 380px;
	float: right;
	box-shadow: 0px 0px 11px 5px #b0b0b0;
	margin-top: -10px;
	z-index: 10;
	position: relative;
}

.inscrit{
	background: #b0b0b0;
	padding: 50px 20px 20px 20px;
}

.inscrit span{
	font-size: 12px;
}

.inscrit input[type=text], .inscrit input[type=password]{
	width: 110px;
	padding-left: 5px;
	height: 20px;
	margin-top: 10px;
}

.inscrit input[type=submit]{
	padding: 3px 5px;
	width: auto;
	background: #25b6ae;
	border: none;
	color: white;
	height: 26px;
	font-family: "biko_bold";
	font-size: 16px;
	cursor: pointer;
}

.inscrit label{
	font-size: 8px;
	height:22px;
	padding-left: 21px;
	line-height: 22px;
	width: 100px;
}

.inscrit a{
	font-size: 11px;
}

.pas-inscrit{
	padding: 0 20px;
}

.pas-inscrit p {
	font-family: "biko_bold";
}

.pas-inscrit p:first-child{
	margin-bottom: 5px;
}

.pas-inscrit span.inscription{
	font-size: 11px;
}

.trait {
	background: url(../images/trait.png) repeat-x 0% 60%;
	text-align: center;
	margin-bottom: 20px;
}

.trait span{
	background: white;
	padding: 0 5px;
}

.pas-inscrit form{
	padding-left: 10px;
	margin-top: 10px;
}

.pas-inscrit form input{
	width: 97%;
	margin-bottom: 10px;
	padding-left: 10px;
}

.pas-inscrit form span{
	font-size: 12px;
}

.pas-inscrit form input:last-of-type{
	margin-top: 10px;
	padding: 5px 10px;
	width: auto;
	background: #25b6ae;
	border: none;
	color: white;
	font-family: "biko_bold";
	font-size: 16px;
}

a{
	color: #25b6ae;
}

/* sidebar */

.sidebar{
	float:left;
	width: 150px;
}
.sidebar ul.menu {
	list-style: none;
	text-transform: uppercase;
	width: 100px;
	padding-left: 0; 
	margin-top: 0;
}

.sidebar ul.menu a{
	text-decoration: none;
}

.sidebar ul.menu li.menu-item, 
.sidebar ul.sub-menu li.sub-menu-item{
	padding-left: 50px;
	height: 45px;
	width: 100%;
	position: relative;
	border-bottom: 2px solid #25b6ae;
	line-height: 45px;
	cursor: pointer;
}

.sidebar ul.menu li.menu-item:hover ul.sub-menu{
	display:block;
}
.sidebar ul.sub-menu{
	display: none;
	position: absolute;
	list-style: none;
	z-index: 3;
	padding-left: 50px; 
	background: white;
	left: 0px;
	width: 100px;
}

.sidebar ul.sub-menu li.sub-menu-item{
	left: -50px;
}

.sidebar .menu-item.infos{
	background: url(../images/infos-inactif.png) 5px 5px no-repeat;
}
.sidebar .menu-item.apprendre{
	background: url(../images/apprendre-inactif.png) 5px 5px no-repeat;
}
.sidebar .menu-item.video{
	background: url(../images/video-inactif.png) 5px 5px no-repeat;
}
.sidebar .menu-item.resultats{
	background: url(../images/resultat-inactif.png) 5px 5px no-repeat;
}

.sidebar .menu-item.infos.active, 
.sidebar .menu-item.infos:hover{
	background: #25b6ae url(../images/infos-actif.png) 5px 5px no-repeat;
}
.sidebar .menu-item.apprendre.active, 
.sidebar .menu-item.apprendre:hover{
	background: #25b6ae url(../images/apprendre-actif.png) 5px 5px no-repeat;
}
.sidebar .menu-item.video.active, 
.sidebar .menu-item.video:hover{
	background: #25b6ae url(../images/video-actif.png) 5px 5px no-repeat;
}
.sidebar .menu-item.resultats.active, 
.sidebar .menu-item.resultats:hover{
	background: #25b6ae url(../images/resultat-actif.png) 5px 5px no-repeat;
}

.sidebar .menu-item.active > a, 
.sidebar .menu-item:hover > a, 
.sidebar .sub-menu-item:hover > a{
	color: #f4f4f4;
}

.sidebar .sub-menu-item:hover{
	background: #25b6ae;
}

.sidebar .menu-home{
	margin-left: 110px;
	margin-top: 10px;
}

.sidebar .menu-home img{
	border: none;
}

/* bloc principal */

.principal{
	width: 828px;
	background: white;
	padding-top: 20px;
	border-left: 2px solid #25b6ae;
	float:left;
}

.principal .reward-block{
	margin: 30px 40px 12px 40px;
	position: relative;
	padding-bottom: 23px;
}

.principal .reward-block div{
	background: #25b6ae;
	margin-left: 30px;
	padding-left: 30px;
	padding-right: 5px;
	height: 30px;
	line-height: 30px;
	text-transform: uppercase;
	display: inline-block;
	font-family: "biko_bold";
	color: white;
}

.principal .reward-block div img{
	width: 60px;
	float: left;
	position: absolute;
	top: -12px;
	left: -5px;

}

.principal .reward-block div .reward-count{
	background: white;
	color: black;
	padding: 0 2px;
}

.principal .player{
	padding: 20px 40px;
}

.principal .menu-onglets{
	display: inline-block;
	text-transform: uppercase;
	border-bottom:2px solid #25b6ae;
	min-width: 490px;
}

.principal .menu-onglet{
	float: left;
	padding: 7px 15px;
	border:2px solid #25b6ae;
	border-left: none;
	border-bottom: none;
	cursor: pointer;
}

.principal .menu-onglet.active, .principal .menu-onglet:hover{
	color: white;
	background-color: #25b6ae;
}

.principal .onglets{
	clear: both;
	display: inline-block;
	float: left;
}
.principal .onglet{
	display: none;
}

.principal .onglet ul{
	list-style: none;
	padding: 0;
}

.principal .onglet ul li{
	clear: both;
	padding: 0 0 20px 40px;
	margin-top: 20px;
	width: 450px;
	height: 86px;
	position: relative;
	border-bottom: 2px solid #25b6ae;
}

.principal .onglet ul li img{
	float: left;
	width: 125px;
	height: 85px;
	margin-right: 10px;
	cursor: pointer;
}

.principal .onglet ul li img.deja-vue {
	position: absolute;
	width: 75px;
	top: 30px;
	left: 65px;
	height: 24px;
	cursor: pointer;
}

.principal .onglet ul li h3{
	margin: 0;
	cursor: pointer;
}
.principal .onglet ul li span{
	line-height: 24px;
}

.bleu{
	color: #0066CC;
	font-family: "biko_bold";
	text-transform: uppercase;
}
.orange{
	color: #FF6600;
	font-family: "biko_bold";
	text-transform: uppercase;
}

.jaune{
	color: #FFCC66;
	font-family: "biko_bold";
	text-transform: uppercase;
}

.principal .onglet ul li span.like {
	background: url(../images/like.png) 0px -1px no-repeat;
	padding-left: 22px;
	background-size: 18px;
	margin-left: 20px;
	margin-right: 10px;
}

.principal .video-conseillees{
	float: left;
	background-color: #25b6ae;
	margin-top: -6px;
	width: 338px;
	padding-top:20px;
}

.principal .video-conseillees h2{
	text-align: center;
	font-size: 22px;
	margin-top: 0;
}

.principal .video-conseillees ul{
	list-style: none;
	padding: 0;
	margin-bottom: 0;
}

.principal .video-conseillees ul li{
	height: 104px;
	padding: 0 20px;
	border-bottom: 2px solid white;
}

.principal .video-conseillees ul li:last-child{
	border-bottom: none;
}

.principal .video-conseillees ul li img{
	width: 125px;
	height: 88px;
	float: left;
	margin-right: 10px;
	border: 1px solid black;
	cursor: pointer;
}

.principal .video-conseillees ul li p{
	color: white;
	line-height: 14px;
	margin: 8px;
}

.principal .video-conseillees ul li p:first-child{
	cursor: pointer;
}


/* login */

.login{
	width: 300px;
	text-align: center;
	margin: 0 auto;
}

.login img {
	margin-top: 125px;
	margin-bottom: 75px;
}

.login label{
	width: 70px;
	display: inline-block;
	text-align: right;
	margin-right: 10px;
	margin-bottom: 10px;
}

.login input[type=submit]{
	color: white;
	margin-left: 8px;
	padding: 5px 10px 5px 10px;
	background-color: #25b6ae;
	text-transform: uppercase;
	border: none;
	display: inline-block;
	cursor: pointer;
}

/* mes infos */
.modifier{
	float: right;
	margin-right: 20px;
	color: #25b6ae;
	cursor: pointer;
}
.general, 
.interets, 
.rewards-info, 
.thematique, 
.apprentissage {
	padding: 20px;
	border-bottom: 2px solid #25b6ae;
}

.general .img-profil{
	float: left;
	width: 175px;
	margin-right: 20px;
	border: 5px solid #25b6ae;
}

.general span {
	line-height: 33px;
	margin-right: 20px;
}

.general .gender {
	height: 21px;
	padding-left: 21px;
	display: inline-block;
	margin-left: 10px;
}	

.general .gender.male{
	background: url(../images/male.png) no-repeat;
	background-size: 20px;
}

.general .gender.female{
	background: url(../images/female.png) no-repeat;
	background-size: 14px;
}

.interets span,
.rewards-info span{
	line-height: 25px;
}

.interets span strong,
.rewards-info span strong{
	color: #25b6ae;
}

.rewards-info .reward-block{
	margin: 30px 40px 12px 0px;
}

.thematique img{
	height: 275px;
	margin-left: 100px;
}
.apprentissage .selecteur-apprentissage{
	margin-left: 80px; 
}

.apprentissage .selecteur-apprentissage span{
	cursor: pointer;
}

.apprentissage .selecteur-apprentissage .active{
	font-weight: bold
}

.apprentissage .imgs-apprentissage img{
	display: none;
}
.apprentissage {
	border: none;
}

.modifier-infos{
	display: none;
}

.modifier-infos input[type=submit]{
	border: none;
	background-color: transparent;
	color: #25b6ae;
	float: right;
	margin-right: 20px;
	cursor: pointer;
	font-family: "biko";
}
.modifier-infos input[type=text],
.modifier-infos input[type=number]{
	padding-left: 5px;
}

.modifier-infos .general span {
	line-height: 37px;
}

.modifier-infos .general label {
	line-height: 37px;
	display: inline-block;
	width: 63px;
}
.modifier-infos .general span.nvscolaire{
	cursor: pointer;
	margin-right: 5px;
}

.modifier-infos .general span.nvscolaire.active{
	color: #25b6ae;
	text-decoration: underline;
}

.modifier-infos .interets{
	border: none;
	margin-bottom: 20px;
}

.modifier-infos .interets textarea{
	margin-top: 5px;
	width: 99%;
	height: 80px;
	padding-left: 5px;
	resize: none;
	font-family: "biko"; 
}

.modifier-infos .interets input[type=submit]{
	color: white;
	background: #25b6ae;
	padding: 5px 10px 5px 10px;
	text-transform: uppercase;
	margin-right: 0px;
}

/* resultats */
.resultat{
	clear: both;
	padding : 35px 20px;
	border-bottom: 2px solid #25b6ae;
}
.resultat .qcm-infos{
	float: left;
	text-align: right;
	width: 300px;
	margin-right: 20px;
}

.resultat .qcm-infos p span{
	color: #25b6ae;
	font-weight: bold;
}

.resultat:last-child{
	border: none;
}