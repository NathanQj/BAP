}

#player_apprendre{
	width: 100%;
	text-align: center;
}
.infos .content{
	background: url(../images/infos.png) 19px 22px no-repeat #333;
	background-size: 50px 59px;
	margin: 0 0 15px 0;
}
.create{
	padding: 0 0 24px 0;
}
.create .content{
	background: url(../images/creer.png) 19px 22px no-repeat #333;
	background-size: 55px;
	
	
}

.content{
	display: inline-block;
	width: 100%;
	padding: 15px 0;
	height: 67px;
	cursor: pointer;
}

.content .profil-incomplet{
	padding-left: 91px;
	color: white;
	display: none;
}

.content .profil-incomplet hr {
	border: 1px solid white;
}

.content h2{
	padding-left: 90px; 
	margin-top: 0px;
	margin-bottom: 10px;
	line-height: 20px;
}

.create h2{
	padding-left: 90px; 
	margin-top: 0px;
	margin-bottom: 10px;
	line-height: 20px;
}

.create span{
	padding-left: 90px;
	display: inherit;
	color: white;
	margin-bottom: 10px;
	height: 57px;
}
.create img{
	width: 340px;
	margin: 0 20px;
}



.partage_points{
	background: url(../images/apprendre.png) 19px 22px no-repeat #333;
	background-size: 50px 59px;
    margin: 60px 0 0;
	height: 339px;
}
.partage_points h2{
	color: #fff;
    font-family: 'Open Sans',sans-serif;
    font-weight: bold;
    margin: 0;
    text-align: center;
    text-transform: uppercase;
}
.partage_points_content{

	padding: 10px;
}
.partage_points_content ul{
	list-style-type: none;
	margin: 0;
	padding: 0;
}
.partage_points_content ul li {
	font-weight: normal;
    line-height: 45px;
    padding: 0 0 0 10px;
    text-align: left;
	color: #fff;
	font-size: 14px;
}
.partage_points_content ul li span{
	color: #25b6ae;
	font-size: 30px;
}
.partage_points_content ul li span:last-child{
	color: #333;
	font-size: 11px;
}

.populaire{
    clear: both;
    padding: 30px 0 10px;
    width: 960px;
}
.populaire h3, .historique h3{
	border-bottom: 1px solid #e4e4e4;
	color: black;
	font-weight: normal;
}
.populaire ul.inline-video li:nth-child(7n){
	margin-right: 0;
}

.populaire ul.inline-video li img{
	border: 1px solid black;
	width: 119px;
	height: 70px;
	
}

.historique{
	clear: both;		
	padding-right: 20px;
}

.historique ul.inline-video li:nth-child(7n){
	margin-right: 0;
}
.historique ul.inline-video li img{
	border: 1px solid black;
	width: 119px;
	height: 70px;
}

ul.inline-video{
    display: inline-block;
