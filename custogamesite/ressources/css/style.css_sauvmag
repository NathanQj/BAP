
@font-face
{
	font-family: biko_light;
	src: url(../font/Biko_Light-Restricted.otf);
}
@font-face
{
	font-family: biko;
	src: url(../font/Biko_Regular.otf);
}

@font-face
{
	font-family: biko;
	src: url(../font/Biko_Bold.otf);
	font-weight: bold;
}
@font-face
{
	font-family: biko_black;
	src: url(../font/Biko_Black.otf);
}

@font-face
{
	font-family: kankin;
	src: url(../font/Kankin.otf);
	src: url(../font/Kankin.ttf);
}

.border-inside
{
	-moz-box-sizing:    border-box;
   -webkit-box-sizing: border-box;
    box-sizing:        border-box;
}

body{
	font-family: "biko", arial;
	text-align: center;
	position: relative;
	background-color: #f9f9f9;
	margin: 0;

}

b{
	font-weight: bold;
}



.main-pre{
	display: inline-block;
	width: 980px;
	margin: 50px auto;
	text-align: left;
	-moz-box-shadow: 0px 0px 6px 0px #c0c0c0;
	-webkit-box-shadow: 0px 0px 6px 0px #c0c0c0;
	-o-box-shadow: 0px 0px 6px 0px #c0c0c0;
	box-shadow: 0px 0px 6px 0px #c0c0c0;
	filter:progid:DXImageTransform.Microsoft.Shadow(color=#c0c0c0, Direction=NaN, Strength=6);
}
.main{
	display: inline-block;
	width: 980px;
	margin:0 auto;
	text-align: left;
	padding-bottom: 10px;
	
}


/* header */
.header{
	display: inline-block;
	width: 980px;
	margin: 0 auto;
	text-align: left;
	padding-top: 20px;
	padding-bottom: 20px;
	font-family: "biko", arial;;
	position: relative;
}
.header div, .header img, .header form{
	float: left;
}

.logo{
	width: 100px;
	border: none;
}
.search{

bottom: 20px;
    left: 180px;
    position: absolute;
}

.loupe{
	width: 20px;
	padding-right: 10px;
}

.search input[type=text]{
	width: 300px;
	height: 24px;
	border: 1px solid #9f9f9f;
    border-radius: 3px;
	background-color: #fff;
	padding: 0 0 0 10px;
}
.search input[type=submit]{
	background-color: #333;
    border: medium none;
    border-radius: 3px;
    color: #fff;
    cursor: pointer;
    font-weight: bold;
    padding: 4px 5px;
}
.citation{
	font-size: 14px;
    left: 120px;
    position: absolute;
    text-align: center;
    top: 20px;
    width: 400px;
}
.header .reward-block{
	bottom: -5px;
    margin: 0;
    position: absolute;
    right: 20px;
}
.header-profil{
	position: absolute;
    right: 20px;
    top: 20px;
}

.header-profil img{
    height: 36px;
    width: 36px;
}

.header-profil a{
	color: #37b6ae;
}

.header-profil p {
	float: left;
	margin-top: 0;
	margin-left: 15px;
}

/* nav */
.nav{ 
	background-color: #333;
    height: 39px;
    margin: 0 auto 40px auto;
    width: 100%;
}
.nav ul{
	height: 39px;
    list-style-type: none;
    margin: 0 auto;
   /*  padding: 10px 0 0; */
    text-align: left;
    width: 980px;
	
}
.nav ul li{
	border-right: 1px solid #fff;
    display: inline-block;
    height: 39px;
    list-style-type: none;
    
}
.nav ul li a{
	color: #fff;
    display: block;
    font-size: 13px;
    font-weight: normal;
    height: 72%;
    padding: 11px 20px 0;
    text-decoration: none;
    text-transform: uppercase;
}
.nav ul li:last-child{
	border-right: none;
}
.nav ul li a:hover{
	color: #333;
	background-color: #fff;
}

/*fin */

/* footer */
.footer{ 
	background-color: #333;
    height: 39px;
    margin: 0 auto 10px auto;
    width: 100%;
}
.footer p{ 
	color: #fff;
    font-size: 11px;
    padding: 10px;
    text-align: center !important;
    vertical-align: middle;
}
.footer ul{
	height: 39px;
    list-style-type: none;
    margin: 0 auto;
   /*  padding: 10px 0 0; */
    text-align: left;
    width: 980px;
	
}
.footer ul li{
    display: inline-block;
    height: 32px;
    list-style-type: none;
    
}
.footer ul li a{
	color: #fff;
    display: block;
    font-size: 11px;
    font-weight: normal;
    height: 88%;
    padding: 11px 20px 0;
    text-decoration: none;
}

.footer ul li a:hover{
	text-decoration: underline;
	
}

/*fin */



/* home */

.col1{
	width: 540px;
	float: left;
	margin-right: 20px;
}

.col2{
	float: left;
	width: 400px;
}

.main h3{
	color: #25b6ae;
	margin: 10px 0;
	font-weight: normal;
	text-transform: uppercase;
}

.main h2{
	color: white;
	text-transform: uppercase;
	font-family: 'Open Sans', sans-serif;		font-weight: bold;
}
.main hr.separator{	border: 1px solid #25b6ae;}
.main hr.bottom{	border: 6px solid #25b6ae;margin-right:20px}
.teaser{
	height: 358px;
	position: relative;
}
.teaser h3{
	font-weight: normal;
    margin-top: 0;
    text-transform: uppercase;
}

.pub1, .pub2{
	width: 100%;
	border: 1px solid black;
	margin-top: 20px;
}
.apprendre{
	padding: 0 0 23px 0
}
.apprendre .content{
	background: url(../images/apprendre.png) 19px 22px no-repeat #333;
