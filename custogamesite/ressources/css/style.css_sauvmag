	margin-top: 20px;
	height: 74px;
	position: relative;
	border-bottom: 1px solid #25b6ae;
	line-height: 18px;		
}

.principal .onglet ul li img{
	float: left;
	width: 120px;
	height: 75px;
	margin-right: 10px;
	cursor: pointer;
}

.principal .onglet ul li img.deja-vue {
	position: absolute;
	width: 65px;
	top: 30px;
	left: 55px;
	height: 19px;
}

.principal .onglet ul li h3{
	margin: 0;
	font-size: 13px;
	cursor: pointer;
	text-transform:uppercase;
}
.principal .onglet ul li span{
	font-size: 12px;
}

.bleu{
	color: #25b6ae;
	font-family: "biko", arial;;
}
.orange{
	color: #FF6600;
	font-family: "biko", arial;;
	text-transform: uppercase;
	font-weight: bold;
}

.jaune{
	color: #FFCC66;
	font-family: "biko", arial;;
	text-transform: uppercase;
	font-weight: bold;
}

.principal .onglet ul li span.like {
	background: url(../images/like.png) 0px -1px no-repeat;
	padding-left: 22px;
	background-size: 14px;
	margin-left: 20px;
	margin-right: 10px;
}

.principal .video-conseillees{
	float: right;
	background-color: #25b6ae;
	margin-top: -5px;
	width: 290px;
	padding-top:10px;
}

.principal .video-conseillees h2{
	text-align: center;
	font-size: 18px;
	margin: 0;
}

.principal .video-conseillees ul{
	list-style: none;
	padding: 0;
	margin: 0;
}

.principal .video-conseillees ul li{
	height: 77px;
	padding: 10px 20px;
	border-bottom: 1px solid white;		color: white;		cursor: pointer;
	
}
.principal .video-conseillees ul li:hover{	color: #25b6ae;		background-color: #EFEFEF; 
	

}

.principal .video-conseillees ul li:last-child{
	border-bottom: none;
}

.principal .video-conseillees ul li img{
	width: 120px;
	height: 75px;
