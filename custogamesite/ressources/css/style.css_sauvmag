	margin-right: 0;
}
ul.inline-video p{
	margin: 0;
}
.content_vu_like{
	bottom: 2px;
    position: absolute;
    width: 92%;
}

.profils h3 {
	font-weight: normal;
    margin-top: 0;
    text-transform: uppercase;
}
.profils ul {
	border: 1px solid black;
	background: white;
	padding: 10px 10px 0 10px;
	height: 303px;
	display: inline-block;
	margin: 0;
}

.profils ul li{
	display: inline-block;
	width: 63px;
	margin-right: 10px;
	list-style: none;
	font-size: 10px;
}

.profils ul li:nth-child(5n){
	margin-right: 0px;
}

.profils ul li img{
	width: 59px;
	padding: 1px;
}

.profils ul li img.connected{
	border: 1px solid #25b6ae;
	padding: 0;
}

.likeus{
	height: 200px;		background: white;
}

.overlay{
	display: none;
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	z-index: 10;
	background-color: rgba(0,0,0,0.5); 
}

.qcm{
	width: 440px;
	position: fixed;
	top: 0;
	left: 50%;
	margin-left: -220px;
	margin-top: 150px;
	background-color: white;
	border: 1px solid #25b6ae;
	z-index: 11;
	text-align: left;
	
	display: none;

}

.qcm form{
	background: #25b6ae;
}

.qcm .page-number{
	text-transform: uppercase;
	padding: 5px 10px 5px 15px;
	background-color: #25b6ae;
	position: relative;
	display: inline;
	top: 35px;
	left: -10px;
}

.qcm-page{
	display: none;
	background: white;
}

.qcm img{
	border-bottom: 1px solid #25b6ae;
	width: 100%;
	margin-top: -20px;
	max-height: 300px;
}

.qcm h3 {
	margin-left: 15px;
	color: #25b6ae;
}

.qcm .checkbox{
	margin-left: 50px;
	margin-bottom: 20px;
}

.qcm .checkbox input, .inscrit input[type=checkbox]{
	display: none;
}

label.check-style{
	background: url(../images/checkbox.png) no-repeat;
	padding-left: 28px;
	margin-bottom: 10px;
	background-position: 0 2px;
	display: inline-block;
	line-height: 20px;
	cursor:pointer; 
	
}

input[type="checkbox"]:checked + label.check-style {
 	background-position: 0 -23px; 
 }
.quizz-progressbar
{
	width: 50%!important;
	border: none!important;
	position: relative;
	left: 25%;
	margin-bottom: 20px;
}
.qcm .next-page, .qcm input[type=submit]{
	color: white;
	margin-left: 180px;
	margin-bottom: 20px;
	padding: 5px 10px 5px 10px;
	background-color: #25b6ae;
	text-transform: uppercase;
	border: none;
	display: inline-block;
	cursor: pointer;
}

div.qcm-pres{
	background: #25b6ae;
	color: white;
	text-align: center;
	padding: 20px 20px 30px 20px;
	cursor: pointer;
}

.qcm-pres p{
	text-align: initial;
}

.qcm-pres span{
	float: right;
}

.qcm-pres img{
	width: 110px;
}

.qcm-pres h2 {
	margin: 0 0 40px 0;
}

.qcm-pres .suivant{
	background: url(../images/suivant.png) 100% 70% no-repeat;
	padding-right: 15px;
	background-size: 9px;
	margin-bottom: 10px;
}


/* pre-home */

.logo.pre-home{
	position: relative;
	top: 50px;
	left: 50px;
	float: left;
	z-index: 10;
}

.slider-img{
	z-index: 1;
	bottom: 0;
	position: absolute;
}

.slider{
	width: 600px;
	float: left;
	position: relative;
	overflow: hidden;
	height: 606px;
	margin-left: -100px;
	background-color: #fff;

}

.connexion{
	width: 380px;
	float: right;
	z-index: 10;
	position: relative;
	font-family: "arial";
}

.inscrit{
	background: #333;
	color: #fff;
	padding: 10px;
}

.inscrit span{
	font-size: 12px;
}

.inscrit input[type=text], .inscrit input[type=password]{
	border: 0 none;
    border-radius: 3px;
    height: 20px;
    margin-top: 10px;
    padding-left: 5px;
    width: 95%;
}

.inscrit input[type=submit]{
	background: none repeat scroll 0 0 #25b6ae;
    border-radius: 3px;
    color: white;
    cursor: pointer;
    font-size: 12px;
    height: 26px;
    margin: 9px 0 0;
    padding: 3px 5px;
    text-transform: uppercase;
    width: auto;
	border: 0;
}

.inscrit label{
	font-size: 11px;
    height: 22px;
    line-height: 22px;
    margin-left: 2px;
    padding-left: 21px;
    width: 137px;
	}

.inscrit a{
	font-size: 11px;
}

.pas-inscrit{
	padding: 0 20px;
}

.pas-inscrit p {
}

.pas-inscrit p:first-child{
	margin-bottom: 0;
	margin-top: 10px;
}

.pas-inscrit p.inscription{
	font-size: 12px;
}

.trait {
	background: url(../images/trait.png) repeat-x 0% 60%;
	text-align: center;
	margin-bottom: 20px;
}

.trait span{
	background: #f9f9f9;
	padding: 0 5px;
}

.pas-inscrit form{
	padding-left: 5px;
	margin-top: 10px;
}

.pas-inscrit form input{
	width: 97%;
	margin-bottom: 10px;
	padding-left: 10px;
	border: 1px solid #9f9f9f;
	border-radius: 3px;
	padding: 3px 0 3px 10px;
}

.pas-inscrit form span{
	font-size: 12px;
}

.pas-inscrit form input:last-of-type{
	margin-top: 10px;
	padding: 5px 10px;
	width: auto;
	background: #25b6ae;
	border: none;
	color: #fff;
	border-radius: 3px;
	font-size: 12px;
	text-transform: uppercase;
	cursor: pointer;
}

.error, .error2{
	font-size: 11px;
	margin: 5px 0 -5px 20px;
	color: red;
	display: none;
}

a{
	color: #25b6ae;
	text-decoration: none;
}

/* sidebar */

.sidebar{
	float:left;
	width: 150px;
}
.sidebar ul.menu {
	list-style: none;
	text-transform: uppercase;
	width: 100px;
	padding-left: 0; 
	margin-top: 0;
}

.sidebar ul.menu a{
	text-decoration: none;
	color: black;
}

.sidebar .menu-item, 
.sidebar .sub-menu-item{
	padding-left: 50px;
	width: 100%;
	border-bottom: 1px solid #25b6ae;
	line-height: 45px;
	cursor: pointer;
	
}

.sidebar ul.sub-menu{
	display: none;
	list-style: none;
	z-index: 3;
	padding-left: 0;
	width: 150px;
	margin-left: -50px;
}

.sidebar .sub-menu-item{
	position: relative;
	padding-left: 40px;
	padding-right:10px;
	background: white;
	width: 100px;
	font-size:0.8em;
	text-align:right;
	
}

.sidebar .menu-item.home{
	background: url(../images/home-inactif.png) 5px 5px no-repeat;
}

.sidebar .menu-item.infos{
	background: url(../images/infos-inactif.png) 5px 5px no-repeat;
}
.sidebar .menu-item.apprendre{
	background: url(../images/apprendre-inactif.png) 5px 5px no-repeat;
}
.sidebar .menu-item.video{
	background: url(../images/video-inactif.png) 5px 5px no-repeat;
}
.sidebar .menu-item.resultats{
	background: url(../images/resultat-inactif.png) 5px 5px no-repeat;
}

.sidebar .menu-item.home:hover{
	background: #25b6ae url(../images/home-actif.png) 5px 5px no-repeat;
}

.sidebar .menu-item.infos.active, 
.sidebar .menu-item.infos:hover{
	background: #25b6ae url(../images/infos-actif.png) 5px 5px no-repeat;
}
.sidebar .menu-item.apprendre.active, 
.sidebar .menu-item.apprendre:hover{
	background: #25b6ae url(../images/apprendre-actif.png) 5px 5px no-repeat;
}
.sidebar .menu-item.video.active, 
.sidebar .menu-item.video:hover{
	background: #25b6ae url(../images/video-actif.png) 5px 5px no-repeat;
}
.sidebar .menu-item.resultats.active, 
.sidebar .menu-item.resultats:hover{
	background: #25b6ae url(../images/resultat-actif.png) 5px 5px no-repeat;
}

.sidebar .menu-item.active > a, 
.sidebar .menu-item:hover > a, 
.sidebar .sub-menu-item:hover > a{
	color: #f4f4f4;
}

.sidebar .sub-menu-item:hover{
	background: #25b6ae;
}

.sidebar ul.sub-menu li.sub-menu-item:last-child{
	border-bottom:none;
}

.sidebar .menu-home{
	margin-left: 110px;
	margin-top: 10px;
}

.sidebar .menu-home img{
	border: none;
}


/* bloc principal */


.nav_select{
	background-color: #333;
    color: #fff;
    padding: 4px 0 15px 10px;
}
.nav_select label{
	display: inline-block;
	width: 20%;
}
.nav_select select .size{
	width: 20%;
}

.principal{
	overflow: hidden;
}

.reward-block{
	margin: 30px 40px 12px 40px;
	position: relative;
	padding-bottom: 23px;
}

.reward-block div{
	background: #25b6ae;
	margin-left: 30px;
	padding-left: 10px;
	padding-right: 5px;
	height: 30px;
	line-height: 32px;
	text-transform: uppercase;
	display: inline-block;
	color: white;
}

.reward-block div img{
	width: 60px;
	float: left;
	position: absolute;
	top: -12px;
	left: -5px;

}

.reward-block div .reward-count{
	background: white;
	color: black;
	padding: 2px 2px 0px 2px
}

.principal .player{
	padding: 20px 0;
}

.principal .section {
	padding: 0 0 10px 15px;
}

.principal .menu-onglets{
	display: inline-block;
	text-transform: uppercase;
	border-bottom:1px solid #25b6ae;
	min-width: 538px;
	font-size: 13px;
}

.principal .menu-onglet{
	float: left;
	padding: 7px 15px;
	border:1px solid #25b6ae;
	border-left: none;
	border-bottom: none;
